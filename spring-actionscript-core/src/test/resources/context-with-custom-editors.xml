<?xml version="1.0" encoding="UTF-8"?>
<objects xmlns="http://www.springactionscript.org/schema/objects"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://www.springactionscript.org/schema/objects http://www.springactionscript.org/schema/objects/spring-actionscript-objects-1.0.xsd ">

  <object class="org.springextensions.actionscript.ioc.factory.config.CustomEditorConfigurer">
    <property name="customEditors">
      <object>
        <property name="org.springextensions.actionscript.objects.testclasses.PhoneNumber">
          <object class="org.springextensions.actionscript.objects.testclasses.PhoneNumberEditor"/>
        </property>
      </object>
    </property>
  </object>

  <object id="phoneNumber1" class="org.springextensions.actionscript.objects.testclasses.PhoneNumber">
    <constructor-arg value="111"/>
    <constructor-arg value="555"/>
    <constructor-arg value="1234"/>
  </object>

  <object id="contact1" class="org.springextensions.actionscript.objects.testclasses.Contact">
    <property name="phoneNumber" ref="phoneNumber1"/>
  </object>

  <object id="contact2" class="org.springextensions.actionscript.objects.testclasses.Contact">
    <property name="phoneNumber">
      <object class="org.springextensions.actionscript.objects.testclasses.PhoneNumber">
        <constructor-arg value="222"/>
        <constructor-arg value="555"/>
        <constructor-arg value="1234"/>
      </object>
    </property>
  </object>

  <object id="contact3" class="org.springextensions.actionscript.objects.testclasses.Contact">
    <property name="phoneNumber" value="333-555-1234"/>
  </object>

</objects>
