<?xml version="1.0" encoding="UTF-8"?>
<objects xmlns="http://www.springactionscript.org/schema/objects"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://www.springactionscript.org/schema/objects http://www.springactionscript.org/schema/objects/spring-actionscript-objects-0.4.xsd ">

  <property file="strings.properties.txt" />

  <object id="string1" class="String">
    <constructor-arg value="${s1}"/>
  </object>

  <object id="string2" class="String">
    <constructor-arg value="${s2}"/>
  </object>

  <object id="string3" class="String">
    <constructor-arg value="${s3}/file.ext?id={0}"/>
  </object>

  <object id="o" class="Object">
    <property name="a">
      <array>
        <value>${s2}</value>
        <value>${s1}</value>
      </array>
    </property>
  </object>

</objects>
